  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'sg_hdcp'",
      "cargo": {
        "args": [
          "build",
          "--bin=sg_hdcp",
          "--package=sg_hdcp"
        ],
        "filter": {
          "name": "sg_hdcp",
          "kind": "bin"
        }
      },
      "env": {
        "RUST_LOG": "sg_hdcp=debug",
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'sg_hdcp'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=sg_hdcp",
          "--package=sg_hdcp"
        ],
        "filter": {
          "name": "sg_hdcp",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}