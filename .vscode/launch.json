  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "app tests",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/.bin/jest",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/test/common/utils/score.diff_options_diff_holes.test.js",
      "args": [
      ],
    },
    {
      "name": "app",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "attach"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "ghin",
      "cwd": "${workspaceFolder}/ghin/",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=${workspaceFolder}/ghin/Cargo.toml",
          "--bin=ghin",
          "--package=ghin"
        ],
        "filter": {
          "name": "ghin",
          "kind": "bin"
        }
      },
      "env": {
        "RUST_LOG": "ghin=debug",
      },
      "args": []
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "ghin debug tests",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=ghin",
          "--package=ghin"
        ],
        "filter": {
          "name": "ghin",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/ghin"
    },
    {
      "name": "api",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "nodemon",
      "runtimeArgs":[
        "--experimental-specifier-resolution=node",
        "${workspaceFolder}/api/src/server.js"
      ],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "api schema_update 20210602",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/api",
      "args": [
        "--es-module-specifier-resolution=node",
        "./util/arango/schema_updates/20210602.js"
      ],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "api schema_update 20221219",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/api",
      "args": [
        "--es-module-specifier-resolution=node",
        "./util/arango/schema_updates/20221219.js"
      ],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "api schema_update 20221220",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/api",
      "args": [
        "--es-module-specifier-resolution=node",
        "./util/arango/schema_updates/20221220.js"
      ],
    }
  ]
}