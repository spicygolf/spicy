{
  "name": "spicygolf",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.0.0-beta.54",
    "babel-bridge": "^1.12.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^5.0.2",
    "detox": "^9.0.6",
    "flow-bin": "^0.67.1",
    "jest": "^23.6.0",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "16.4.1"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone X\"",
    "test": "jest",
    "flow": "flow; test $? -eq 1 -o $? -eq 2"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/spicygolf.app",
        "build": "xcodebuild -project ios/spicygolf.xcodeproj -scheme spicygolf -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -UseModernBuildSystem=NO",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  },
  "dependencies": {
    "apollo-cache-hermes": "^0.8.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.4.7",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.20.1",
    "react": "^16.4.2",
    "react-apollo": "^2.1.8",
    "react-logger": "^1.1.0",
    "react-native": "^0.56.0",
    "react-native-dropdownalert": "^3.7.1",
    "react-native-elements": "^0.19.1",
    "react-native-iphone-x-helper": "^1.0.1",
    "react-native-router-flux": "^4.0.5",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.18.2",
    "tcomb-form-native": "^0.6.15"
  }
}
