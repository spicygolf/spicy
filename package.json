{
  "name": "spicygolf",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-jest": "24.8.0",
    "detox": "^12.11.0",
    "flow-bin": "^0.100.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-test-renderer": "16.8.3"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone X\"",
    "test": "jest",
    "flow": "flow; test $? -eq 1 -o $? -eq 2"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/spicygolf.app",
        "build": "xcodebuild -project ios/spicygolf.xcodeproj -scheme spicygolf -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -UseModernBuildSystem=NO",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0",
    "@react-native-community/async-storage": "^1.5.0",
    "apollo-cache-hermes": "^0.8.9",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.2",
    "apollo-link-context": "^1.0.17",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.13",
    "moment": "^2.24.0",
    "react": "16.8.3",
    "react-apollo": "^2.5.6",
    "react-logger": "^1.1.0",
    "react-native": "0.59.9",
    "react-native-dropdownalert": "^4.0.1",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-vector-icons": "^6.5.0",
    "react-navigation": "^3.11.0",
    "tcomb-form-native": "^0.6.20"
  }
}
